{
	"name": "competitive-programming-helper",
	"displayName": "Competitive Programming Helper",
	"description": "Run test cases easily.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.runCodeforcesTestcases"
	],
	"main": "./extension.js",
