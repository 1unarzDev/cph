{
  "name": "competitive-programming-helper",
  "displayName": "Competitive Programming Helper",
  "description": "Makes downloading running Codeforces testcases or general competitve programming easy.",
  "keywords": [
    "competitive",
    "codeforces",
    "icpc",
    "acm",
    "c++",
    "testcases"
  ],
"version": "0.0.9",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.39.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "publisher": "DivyanshuAgrawal",
  "main": "dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.runCodeforcesTestcases",
        "title": "Run Codeforces Testcases",
        "icon": "icon.png",
        "enablement": "resourceExtname==.cpp || resourceExtname ==.c++ || resourceExtname == .cc",
        "category": "Competitive"
      },
      {
        "command": "extension.openTestcaseFile",
        "title": "Open the testcases for the current C++ file",
        "icon": "icon.png",
        "enablement": "resourceExtname==.cpp || resourceExtname ==.c++ || resourceExtname == .cc",
        "category": "Competitive"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+alt+b",
        "command": "extension.runCodeforcesTestcases",
        "when": "resourceExtname==.cpp || resourceExtname ==.c++ || resourceExtname == .cc"
      },
      {
        "key": "ctrl+alt+0",
        "command": "extension.openTestcaseFile",
        "when": "resourceExtname==.cpp || resourceExtname ==.c++ || resourceExtname == .cc"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agrawal-d/competitive-programming-helper"
  },
  "scripts": {
    "test": "node ./test/runTest.js",
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.39.0",
    "eslint": "^5.13.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "ts-loader": "^6.2.1",
    "typescript": "^3.3.1",
    "vscode-test": "^1.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  }
}
